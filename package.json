  "description": "ðŸš€ Arco Design Pro Vue is a production-ready front-end solution for admin interfaces. Based on Vue 3, TypeScript, Vite, and Arco Design.",
  "keywords": [
    "vue",
    "typescript",
    "vue3",
    "ts",
    "tsx",
    "admin",
    "arco-design",
    "arco-design-pro-vue",
    "arco-design-pro"
  ],
  "homepage": "https://github.com/LLiuHuan/arco-design-pro-vue",
  "bugs": {
    "url": "https://github.com/LLiuHuan/arco-design-pro-vue/issues",
    "email": "184555556@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LLiuHuan/arco-design-pro-vue.git"
  },
  "license": "MIT",
  "author": {
    "name": "LLiuHuan",
    "email": "184555556@qq.com",
    "url": "https://github.com/LLiuHuan/arco-design-pro-vue"
  },
  "type": "module",
  "website": "https://lliuhuan.github.io/arco-design-pro-vue",
  "documentation": "https://github.com/LLiuHuan/arco-design-pro-vue",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 turbo build",
    "build:analyze": "turbo build:analyze",
    "build:antd": "pnpm run build --filter=@arco/web-antd",
    "build:docker": "./scripts/deploy/build-local-docker-image.sh",
    "build:docs": "pnpm run build --filter=@arco/docs",
    "build:ele": "pnpm run build --filter=@arco/web-ele",
    "build:naive": "pnpm run build --filter=@arco/web-naive",
    "build:play": "pnpm run build --filter=@arco/playground",
    "changeset": "pnpm exec changeset",
    "check": "pnpm run check:circular && pnpm run check:dep && pnpm run check:type && pnpm check:cspell",
    "check:circular": "vsh check-circular",
    "check:cspell": "cspell lint **/*.ts **/README.md .changeset/*.md --no-progress",
    "check:dep": "vsh check-dep",
    "check:type": "turbo run typecheck",
    "clean": "node ./scripts/clean.mjs",
    "commit": "czg",
    "dev": "turbo-run dev",
    "dev:antd": "pnpm -F @arco/web-antd run dev",
    "dev:docs": "pnpm -F @arco/docs run dev",
    "dev:ele": "pnpm -F @arco/web-ele run dev",
    "dev:naive": "pnpm -F @arco/web-naive run dev",
    "dev:arco": "pnpm -F @arco/web-arco run dev",
    "dev:play": "pnpm -F @arco/playground run dev",
    "format": "vsh lint --format",
    "lint": "vsh lint",
    "postinstall": "pnpm -r run stub --if-present",
    "preinstall": "npx only-allow pnpm",
    "prepare": "is-ci || husky",
    "preview": "turbo-run preview",
    "publint": "vsh publint",
    "reinstall": "pnpm clean --del-lock && pnpm install",
    "test:unit": "vitest run --dom",
    "test:e2e": "turbo run test:e2e",
    "update:deps": "npx taze -r -w",
    "version": "pnpm exec changeset version && pnpm install --no-frozen-lockfile"
  },
  "devDependencies": {
    "@arco/commitlint-config": "workspace:*",
    "@arco/eslint-config": "workspace:*",
    "@arco/prettier-config": "workspace:*",
    "@arco/stylelint-config": "workspace:*",
    "@arco/tsconfig": "workspace:*",
    "@arco/uno-config": "workspace:*",
    "@arco/vite-config": "workspace:*",
    "@changesets/changelog-github": "catalog:",
    "@changesets/cli": "catalog:",
    "@playwright/test": "catalog:",
    "@types/node": "catalog:",
    "@unocss/postcss": "catalog:",
    "@unocss/preset-wind4": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vue/test-utils": "catalog:",
    "autoprefixer": "catalog:",
    "cross-env": "catalog:",
    "cspell": "catalog:",
    "happy-dom": "catalog:",
    "husky": "catalog:",
    "is-ci": "catalog:",
    "lint-staged": "catalog:",
    "playwright": "catalog:",
    "rimraf": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unocss": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.12.0"
  },
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "*"
      }
    },
    "overrides": {},
    "neverBuiltDependencies": [
      "canvas",
      "node-gyp"
    ]
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
